
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.6.3.js"></script>
    <script src="~/Script/StateMaster.js"></script>
    <script src="~/Script/AutoComplete.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.16/jquery-ui.js"></script>
    <link href="https://code.jquery.com/ui/1.10.4/themes/ui-lightness/jquery-ui.css"
          rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <title>StateMaster</title>
    <style>
    </style>
</head>
<body>
    <form method="post" id="MyForm" autocomplete="off" >
        <div class="row">
            <div class="col-sm-1"></div>
            <div class="col-sm-10 border border-2 rounded border-top-0 pb-2">
                <div class="row fs-5">
                    <div class="col-sm-2">
                        <a href="/Master/CountryMaster"> Country Master </a>
                    </div>
                    <div class="col-sm-2 float-end">
                        <a href="/Master/StateMaster"> State Master </a>
                    </div>
                </div><br />
                <input type="hidden" id="hstID" value="0" />
                <div class="row text-bg-primary rounded-top" id="state">State Master</div>
                <div class="row p-2" id="mid">
                    <div class="col-sm-6 autocomplete">
                        <lable style="margin-left:25px;">Country</lable> <span style="color:red;">*</span>
                        <input type="text" id="txtCountry" class="form-control" name="Country" placeholder="Country" onfocus="AutoComplete('txtCountry','GetCountry')" />
                        <lable>State Code</lable> <span style="color:red;">*</span>
                        <input type="text" id="txtStateCode" class="form-control" name="StateCode" placeholder="StateCode" />

                        <lable>State Name</lable> <span style="color:red;">*</span>
                        <input type="text" id="txtStateName" class="form-control" name="StateName" placeholder="StateName " />
                    </div>
                    <div class="col-sm-6">

                        <input type="checkbox" id="IsActive" />Active
                    </div>
                    <div class="col-sm-12">

                    </div>
                    <div class="col-sm-12">

                    </div>
                </div>
                <div class="row" id="footer">
                    <div class="col-sm-2"></div>
                    <div class="col-sm-8 text-center">
                        <button type="button" id="btnSave" class="btn btn-outline-success rounded-5">Save</button>
                        <button type="reset" id="btnReset" class="btn btn-outline-danger rounded-5">Reset</button>
                    </div>
                    <div class="col-sm-2"></div>

                </div>
            </div>
            <div class="col-sm-1"></div>
        </div>
        <div class="row mt-2">
            <div class="col-sm-1"></div>
            <div class="col-sm-10">
                <nav class="navbar navbar-light" style="background: #005c99; border-radius:6px 6px 0px 0px">
                    <b><i id="lblTotal"></i></b>
                    <div class="d-flex">
                       
                    </div>
                </nav>
                <div id="dvGrid" class="row ms-0 w-100" style="text-align:center;">
                </div>
            </div>
            <div class="col-sm-1"></div>
        </div>
    </form>
</body>
</html>
